{
  "name": "fudan-acm-homepage",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "3.24.2",
    "cookie-parser": "~1.4.3",
    "debug": "4.1.1",
    "express": "^4.15.4",
    "express-cors": "0.0.3",
    "express-session": "^1.15.2",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-markdown": "^4.2.2",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0"
  },
  "eslintConfig": {
